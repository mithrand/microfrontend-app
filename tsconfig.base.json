{
  "compilerOptions": {
      "moduleResolution": "node",
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["es2015", "es2016", "es2017", "dom"],
      "strict": true,
      "sourceMap": true,
      "declaration": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
  },
}
